
public class ExercicioDois {
	public static boolean possoIrAoBanco ( String dia, int hora) {
		
		boolean diaSemana = (dia == "Segunda" || dia == "Terça" || dia == "Quarta" || dia == "Quinta" || dia =="Sexta" || dia == "Sábado" || dia == "Domingo"); 
		
		return(diaSemana);
		
		boolean horaDia = (hora == "23" || hora == "22" ||hora == "21" ||hora == "20" ||hora == "19" ||hora == "18" ||hora == "17" ||hora == "16" ||hora == "15" ||hora == "14" ||hora == "13" ||hora == "12" ||hora == "11" ||hora == "10" ||hora == "9" ||hora == "8" ||hora == "7" ||hora == "6" ||hora == "5" ||hora == "4" ||hora == "3" ||hora == "2" ||hora == "1" ||hora == "00");
		
				
				if(dia.equalsIgnoreCase("domingo") || dia.equalsIgnoreCase("sabado")) {
					
					return false;
					
				}
				else{
					return true;
				}
	}
	public static void main(String[] args) {
		
		System.out.println(possoIrAoBanco("domingo , 14"));
	}
}
